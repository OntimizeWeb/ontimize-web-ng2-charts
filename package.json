{
  "name": "ontimize-web-ngx-charts",
  "version": "3.0.0-rc.0",
  "description": "An implementation of nvD3 for Ontimize Web Ng2",
  "scripts": {
    "preinstall": "npm config set package-lock false",
    "rimraf": "rimraf",
    "webpack": "webpack",
    "test": "karma start",
    "clean": "rimraf dist tmp",
    "init-common": "npm run clean && gulp inline-templates",
    "end-common": "gulp copy-files && gulp charts.styles",
    "webpack:dev": "npm run init-common && webpack --config config/webpack.dev.js --progress && npm run end-common",
    "webpack:prod": "npm run init-common && webpack --config config/webpack.prod.js --progress && npm run end-common",
    "aot:compile": "node_modules/.bin/ngc -p tsconfig.aot.json",
    "aot": "npm run init-common && npm run aot:compile && npm run end-common"
  },
  "keywords": [
    "angular2",
    "ontimize-web-ngx",
    "ontimize-web-ngx-charts",
    "charts",
    "d3",
    "nvD3"
  ],
  "author": "IMATIA S.L.",
  "repository": {
    "type": "git",
    "url": "git@github.com:OntimizeWebf/ontimize-web-ngx-charts.git"
  },
  "license": "Apache-2.0",
  "main": "./index.js",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "dependencies": {
    "d3": "^3.5.6",
    "ng2-nvd3": "git+https://github.com/daominhsangvn/ng2-nvd3.git",
    "nvd3": "^1.8.4"
  },
  "peerDependencies": {
    "core-js": "2.5.3",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "ontimize-web-ngx": "3.0.0-rc.0",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/material": "5.2.4",
    "@angular/material-moment-adapter": "5.2.4",
    "@angular/cdk": "5.2.4",
    "@angular/animations": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/platform-server": "5.2.9",
    "@angular/router": "5.2.9",
    "@angular/upgrade": "5.2.9",
    "@angular/cli": "1.7.3",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "core-js": "2.5.3",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20",
    "reflect-metadata": "0.1.12",
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.46",
    "@types/node": "7.0.10",
    "@types/source-map": "0.5.7",
    "@types/uglify-js": "2.6.30",
    "@types/webpack": "4.1.1",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "ngc-webpack": "4.1.2",
    "node-sass": "4.7.2",
    "rimraf": "2.6.2",
    "tslint": "5.2.0",
    "tslint-loader": "3.5.3",
    "gulp": "3.9.1",
